<!-- Staff Info Results Modals -->
<div
	class="modal fade"
	id="modal-view-staff-info"
	role="dialog"
	aria-hidden="true"
>
	<div class="modal-content">
		<div
			class="close-modal"
			data-dismiss="modal"
		>
			<div class="lr">
				<div class="rl"></div>
			</div>
		</div>
		<div class="container">
			<div class="row style='margin: auto'">
				<div class="col-lg-12">
					<div
						class="modal-body"
						style='margin: auto'
					>
						<h4 id='h4-view-staff-info-title'>TITLE</h4>
						<table  cellpadding='20px'>
							<tbody>
								<tr>
									<td rowspan="7"><img id='img-info-portrait' /></td>
									<td></td>
									<td></td>
								</tr>
								<tr>
									<td></td>
									<td>Name & Last Name :</td>
									<td><span id='span-info-name'></span></td>
								</tr>
								<tr>
									<td></td>
									<td>E-mail :</td>
									<td><span id='span-info-email'></span></td>
								</tr>
								<tr>
									<td></td>
									<td>Tel. :</td>
									<td><span id='span-info-tel'></span></td>
								</tr>
								<tr>
									<td></td>
									<td>Department :</td>
									<td><span id='span-info-division'></span></td>
								</tr>
								<tr>
									<td></td>
									<td>Position :</td>
									<td><span id='span-info-position'></span></td>
								</tr>
								<tr>
									<td></td>
									<td>Manager :</td>
									<td><span id='span-info-mng'></span></td>
								</tr>
								<tr>
									<td></td>
									<td></td>
									<td><button
											style='margin-right: 1%'
											type="button"
											class="btn btn-default btn-close-modal"
											data-dismiss="modal"
										>
											<i class="fa fa-times"></i> Close
										</button></td>
								</tr>
							</tbody>
						</table>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>